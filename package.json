{
  "name": "fastify-prisma-ts",
  "version": "1.0.0",
  "private": true,
  "description": "Basic CRUD application with Fastify, Prisma and Typescript",
  "keywords": [
    "crud",
    "fastify",
    "prisma",
    "typescript"
  ],
  "author": "Marcelo Knob",
  "license": "MIT",
  "type": "module",
  "module": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "lint": "eslint src --ext .ts --fix",
    "prisma:mm": "prisma migrate dev",
    "prisma:md": "prisma migrate dev --name diff --create-only",
    "prisma:mr": "prisma migrate reset",
    "prisma:p": "prisma db pull",
    "prisma:v": "prisma studio"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.1",
    "@fastify/env": "^4.3.0",
    "@fastify/jwt": "^7.2.2",
    "@prisma/client": "5.5.2",
    "fastify": "^4.24.3",
    "fastify-favicon": "^4.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.5",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "prisma": "^5.5.2",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  }
}
