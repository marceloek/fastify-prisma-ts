{
  "name": "fastify-prisma-ts",
  "version": "1.0.0",
  "description": "Basic CRUD application with Fastify, Prisma and Typescript",
  "type": "module",
  "module": "src/server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "lint": "eslint src --ext .ts --fix",
    "prisma:md": "prisma migrate dev --name migration --create-only",
    "prisma:mm": "prisma migrate dev --name migration",
    "prisma:ms": "prisma migrate reset",
    "prisma:pull": "prisma migrate reset",
    "prisma:v": "prisma studio"
  },
  "keywords": [
    "crud",
    "fastify",
    "prisma",
    "typescript"
  ],
  "author": "Marcelo Knob",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.5",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.1",
    "@fastify/env": "^4.3.0",
    "@fastify/jwt": "^7.2.2",
    "@prisma/client": "^5.5.2",
    "fastify": "^4.24.3",
    "fastify-favicon": "^4.3.0",
    "zod": "^3.22.4"
  }
}
